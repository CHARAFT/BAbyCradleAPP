<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Define the corners of the shape -->
    <corners android:radius="50dp" />

    <!-- Define the solid color of the shape -->
    <solid android:color="#E0DAEF" />

    <!-- Optionally, define a stroke (border) for the shape -->
    <stroke
        android:width="2dp"
        android:color="#ECEDED" />
</shape>